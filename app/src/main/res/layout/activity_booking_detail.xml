                                android:layout_marginLeft="@dimen/dimen_10"
                                android:layout_marginRight="@dimen/dimen_10"
                                android:layout_marginTop="@dimen/dimen_10"
                                android:background="@color/slidingBorderColor" />

                            <TextView
                                android:id="@+id/tv_pickup_details_label"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/view_recurring1"
                                android:layout_marginLeft="@dimen/dimen_10"
                                android:layout_marginRight="@dimen/dimen_10"
                                android:layout_marginTop="@dimen/dimen_10"
                                android:text="@string/pickuptimecolon" />

                            <TextView
                                android:id="@+id/tv_pickup_details"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_below="@+id/view_recurring1"
                                android:layout_marginLeft="@dimen/dimen_10"
                                android:layout_marginRight="@dimen/dimen_10"
                                android:layout_marginTop="@dimen/dimen_10"
                                android:text=""
                                android:textStyle="bold" />

                            <View
                                android:id="@+id/view_recurring2"
                                android:layout_width="match_parent"
                                android:layout_height="0.8dp"
                                android:layout_below="@+id/tv_pickup_details_label"
                                android:layout_marginBottom="@dimen/dimen_5"
                                android:layout_marginLeft="@dimen/dimen_10"
                                android:layout_marginRight="@dimen/dimen_10"
                                android:layout_marginTop="@dimen/dimen_10"
                                android:background="@color/slidingBorderColor" />

                            <TextView
                                android:id="@+id/tv_delivery_details_label"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/view_recurring2"
                                android:layout_marginLeft="@dimen/dimen_10"
                                android:layout_marginRight="@dimen/dimen_10"
                                android:layout_marginTop="@dimen/dimen_10"
                                android:text="@string/deliverytimecolon" />

                            <TextView
                                android:id="@+id/tv_delivery_details"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_below="@+id/view_recurring2"
                                android:layout_marginLeft="@dimen/dimen_10"
                                android:layout_marginRight="@dimen/dimen_10"
                                android:layout_marginTop="@dimen/dimen_10"
                                android:text=""
                                android:textStyle="bold" />

                        </RelativeLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="@dimen/dimen_15"
                            android:background="@color/viewColor" />


                        <!--Package detail-->
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
