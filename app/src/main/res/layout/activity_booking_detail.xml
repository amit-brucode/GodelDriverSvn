                                    android:textColor="@color/colorPrimary"
                                    android:textStyle="bold" />

                                <TextView
                                    android:id="@+id/tv_package_id"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentRight="true"
                                    android:layout_gravity="right|end"
                                    android:layout_marginLeft="10dp"
                                    android:layout_toRightOf="@+id/tv_booking_timing_label"
                                    android:gravity="right|end"
                                    android:text="@string/bookingidcolon"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="@dimen/font_normal" />
                            </RelativeLayout>

                            <TextView
                                android:id="@+id/tv_pickup_label"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/lay1"
                                android:layout_marginTop="@dimen/dimen_15"
                                android:text="@string/pickupcolonn" />

                            <TextView
                                android:id="@+id/tv_package_pickup_time"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_below="@+id/lay1"
                                android:layout_marginTop="@dimen/dimen_15"
                                android:textColor="@color/blackColor"
                                android:textStyle="bold" />

                            <View
                                android:id="@+id/view_booking1"
                                android:layout_width="match_parent"
                                android:layout_height="0.8dp"
                                android:layout_below="@+id/tv_pickup_label"
                                android:layout_marginTop="@dimen/dimen_10"
                                android:background="@color/slidingBorderColor" />


                            <TextView
                                android:id="@+id/tv_delivery_label"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/view_booking1"
                                android:layout_marginTop="@dimen/dimen_10"
                                android:text="@string/deliverycolonn" />

                            <TextView
                                android:id="@+id/tv_package_delivery_time"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_below="@+id/view_booking1"
                                android:layout_marginTop="@dimen/dimen_10"
                                android:textColor="@color/blackColor"
                                android:textStyle="bold" />
<View
                                android:id="@+id/view_booking23"
                                android:layout_width="match_parent"
                                android:layout_height="0.8dp"
                                android:layout_below="@+id/tv_delivery_label"
                                android:layout_marginTop="@dimen/dimen_10"
                                android:background="@color/slidingBorderColor" />


                            <TextView
