                            android:layout_height="wrap_content"
                            android:padding="@dimen/dimen_10">

                            <RelativeLayout
                                android:id="@+id/lay1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:id="@+id/tv_booking_timing_label"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
