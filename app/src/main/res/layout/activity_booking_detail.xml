                                android:layout_height="@dimen/dimen_45"
                                android:layout_below="@+id/tv_status_label"
                                android:layout_marginTop="@dimen/dimen_30"
                                android:background="@drawable/btn_check_route"
                                android:shadowColor="#FFD337"
                                android:shadowDx="10"
                                android:shadowDy="10"
                                android:text="@string/checkroutebutton"
                                android:textColor="#045695"
                                android:visibility="gone" />

                        </RelativeLayout>

                        <RelativeLayout
